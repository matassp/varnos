@model IEnumerable<Varnos.Models.Regions>

@{
    ViewData["Title"] = "Admin - Regions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="box box--center" style="max-width: 1000px; margin-top: 1em;">
    <h2>Regions</h2>
    <table class="table">
        <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Latitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Longitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Zoom)
            </th>
            <th></th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Latitude)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Longitude)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Zoom)
                </td>
            </tr>
        }
        </tbody>
    </table>
</div>
